<?xml version='1.0' encoding='utf-8'?>
<widget id="de.tuerkeireiseblog.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>TÃ¼rkeireise</name>
    <description>
        A basic Framework7 template for PhoneGap.
    </description>
    <author email="mach@dynamicwebsolutions.at" href="http://www.dynamicwebsolutions.at">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <preference name="android-minSdkVersion" value="14" />
        <allow-intent href="market:*" />
		
              <icon src="res/mipmap-mdpi/ic_launcher.png" density="mdpi" />
              <icon src="res/mipmap-hdpi/ic_launcher.png" density="hdpi" />
              <icon src="res/mipmap-xhdpi/ic_launcher.png" density="xhdpi" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="BackupWebStorage" value="none" />
    </platform>
    <preference name="DisallowOverscroll" value="true" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.0" />
    <plugin name="cordova-plugin-console" spec="~1.0.1" />
    <plugin name="cordova-plugin-statusbar" spec="~1.0.1" />
	

</widget>
