<?xml version='1.0' encoding='utf-8'?>
<widget id="de.tuerkeireiseblog.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Türkeireise</name>
    <description>
        Alle Informationen für deinen Türkeiurlaub
    </description>
    <author email="mach@dynamicwebsolutions.at" href="http://www.dynamicwebsolutions.at">
       DWS GmbH
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
